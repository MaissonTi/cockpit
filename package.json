{
  "name": "my-turborepo",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "type-check": "turbo type-check --concurrency=70% --affected",
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "test:e2e": "turbo test:e2e",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "gencc": "turbo gencc",
    "clear": "pnpm store prune && find . -name 'dist' -type d -prune -exec rm -rf '{}' + && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "prisma:init": "cd apps/api && pnpm run prisma:init"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "@nestjs/swagger": "^7.3.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "prettier": "^3.2.5",
    "turbo": "^2.3.1"
  },
  "packageManager": "pnpm@8.15.5",
  "engines": {
    "node": ">=18"
  }
}
